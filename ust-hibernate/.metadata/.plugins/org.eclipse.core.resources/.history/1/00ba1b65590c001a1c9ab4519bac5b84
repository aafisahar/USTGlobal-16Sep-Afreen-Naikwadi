package com.ustglobal.jpawithhibernateapp.onetoone;

import javax.persistence.*;

@Entity
@Table(name="person")
public class Person {

	@Column
	@Id
	private int pid;
	
	@Column
	private String pname;

	public int getPid() {
		return pid;
	}

	public void setPid(int pid) {
		this.pid = pid;
	}

	public String getPname() {
		return pname;
	}

	public void setPname(String pname) {
		this.pname = pname;
	}
	
}
